{
  "expo": {
    "name": "react-native",
    "slug": "react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/react-native.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/react-native.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/react-native.svg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/react-native.svg"
    },
    "description": "Mobile application for Android and iOS written in React Native",
    "githubUrl": "https://github.com/SergiusNahnoinyi/react-native-project"
  }
}
